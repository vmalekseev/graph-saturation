<Window x:Class="MainProject.NamiingGraphDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MainProject"
        mc:Ignorable="d"
         Height="200" Width="300" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid Background="#222E31">
        <StackPanel>
            <Label HorizontalAlignment="Center" VerticalContentAlignment="Center" FontSize="18" Foreground="WhiteSmoke" HorizontalContentAlignment="Center">ДАЙТЕ НАЗВАНИЕ ГРАФУ</Label>
            <TextBox BorderThickness="1" BorderBrush="WhiteSmoke" Foreground="WhiteSmoke" x:Name="textBox" Height="30" FontSize="18" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Margin="10,0"></TextBox>
        </StackPanel>
        <Button Content="Сохранить" Click="OnSaveButtonClick" Height="70" FontSize="28" Margin="10,71,10,30"></Button>
    </Grid>
</Window>
